{
    "name": "topRedis",
    "module" : "caf_components#supervisor",
    "description" : "Supervisor for redis",
    "env" : {
        "dieDelay" : -1,
        "interval" : 1000,
        "maxHangRetries": 10,
        "maxRetries" : 5,
        "retryDelay" : 100,
        "appName" : "process.env.APP_NAME||foo-users"
    },
    "components":[
        {
            "name": "log",
            "module" : "caf_components#plug_log",
            "description" : "Logger",
            "env" : {
                "logLevel" : "DEBUG"
            }
        },
        {
            "name": "cp",
            "module" : "../../index.js#plug",
            "description" : "redisPlug Users",
            "env" : {
                "defaultUnits" : 3,
                "defaultTimePerUnit" : 0.000173611,
                "appWriterFraction" : 0.33,
                "appPublishCost" :1,
                "appName" : "$._.env.appName",
                "paas" : "redis",
                "redis" : {
                    "password" : null,
                    "hostname" : "localhost",
                    "port" : 6379
                },
                "coalescing" : {
                    "interval" : 10,
                    "maxPendingUpdates" : 10
                }
            }
        },
        {
            "name": "users",
            "module" : "./ChildUsersImpl",
            "description" : "users",
            "env" : {
                "message" : "child2",
                "number" : 9,
                "leaseTimeout": 1
            }
        }
    ]
}
